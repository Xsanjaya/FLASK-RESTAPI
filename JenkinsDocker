pipeline {
  agent {
    docker { image 'maven:3.3.3' }
  }
  stages {
    stage('PREPARATION') {
      steps {
        sh '''
        whoami
        mvn --version
        mvn clean install
        #docker build -t flask-api:1.1 .
        '''
      }
    }
    stage('Runing APP') {
      steps {
        sh '''
        #docker run -d -p 5000:5000 flask-api:1.1
        '''
      }
    }
  }  
}
