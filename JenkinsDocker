pipeline {
  agent {
    label 'Linux1'
  }
  stages {
    stage('PREPARATION') {
      steps {
        sh '''
        whoami
        docker-compose build
        '''
      }
    }
    stage('Runing APP') {
      steps {
        sh '''
        docker-compose down
        docker-compose up -d
        '''
      }
    }
  }  
}
